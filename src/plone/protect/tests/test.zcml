<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:i18n="http://namespaces.zope.org/i18n"
    i18n_domain="collective.protected"
    >

  <browser:page
      name="test-unprotected"
      for="*"
      class="plone.protect.testing.TestUnprotectedView"
      permission="zope2.View"
      />

  <browser:page
      name="test-safetowrite"
      for="*"
      class="plone.protect.testing.TestSafeToWriteObject"
      permission="zope2.View"
      />

</configure>
